{
  "name": "cycle-boilerplate",
  "version": "1.0.0",
  "description": "Cycle.js boileplate with webpack",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/favicon.ico dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel validate-webpack:*",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jos√© Luis Lafuente",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-stage-2": "^6.11.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "npm-run-all": "^2.3.0",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.2"
  },
  "dependencies": {
    "@cycle/dom": "^10.0.5",
    "@cycle/rxjs-run": "^3.0.2",
    "normalize.css": "^4.2.0",
    "rxjs": "^5.0.0-beta.10",
    "xstream": "^5.1.4"
  }
}
