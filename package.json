{
  "name": "cycle-boilerplate",
  "version": "1.0.0",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "env": {
      "test": {}
    }
  },
  "description": "Cycle.js boileplate with webpack",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/favicon.ico dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dashboard -- webpack-dev-server --env.dev --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack-dashboard -- webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel validate-webpack:*",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jos√© Luis Lafuente",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "html-webpack-plugin": "^2.22.0",
    "npm-check-updates": "^2.8.0",
    "npm-run-all": "^3.0.0",
    "postcss": "^5.1.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.10.1",
    "postcss-reporter": "^1.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.21",
    "webpack-config-utils": "^2.0.0",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "@cycle/dom": "12.2.1",
    "@cycle/isolate": "1.4.0",
    "@cycle/rxjs-run": "3.0.3",
    "normalize.css": "4.2.0",
    "rxjs": "5.0.0-beta.11",
    "xstream": "6.1.0"
  }
}
